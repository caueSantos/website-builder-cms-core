<!doctype html><html lang="pt-br"><head>  <base href="{$app->Url_cliente}">  <link rel="shortcut icon" href="{$assets}imagens/favicon.ico" type="image/x-icon"/>  {include file=$CAMINHO_TPL|cat:'blocos/global/meta_tags.tpl'}  {*ESTILOS*}  {include file=$CAMINHO_TPL|cat:'blocos/global/styles.tpl'}  {$app_json=json_encode($app)}  <script>    window.app = '{$app_json}';    window.appUrl = '{$app->Url_cliente}';    window.utils = {      paginaAtual: '{$pagina_atual}',      painel: '{$painel}',      assets: '{$assets}',      segmentos: ['{$pagina_atual}', '{$segment2}', '{$segment3}', '{$segment4}']    };  </script>  {$Scripts_header_txa}</head><body class="bg-body-light"><main class="pb-60">  <div class="container">    <div class="row justify-content-center">      <div class="col-12 col-lg-6 pt-50">        <div class="logo mx-auto text-center">          <img style="height: 50px; width: auto; max-width: 100%" src="{$assets}imagens/{$app->Logo_txa}"               class="img-fluid d-inline-block" alt="{$app->Nome_app_txf}">        </div>        <div class="box-1 bs-1 br-2 pl-40 pr-40 pt-40 pb-40 text-center mt-30">          <div class="icone fz-70 text-primary lh-0">            <i class="fad fa-construction"></i>          </div>          <h1 class="title fw-700 fz-26 mt-20">            Este site está em construção.<br>            Volte em breve e veja as novidades!          </h1>          <hr class="mt-30 mb-30"/>          <div class="contato text-left">            <h2 class="fz-18 fw-700 ff-secondary">              Contato            </h2>            {if $cliente->Telefone_txf}            <div class="telefone mt-5">              Telefone: {$cliente->Telefone_txf}            </div>            {/if}            {if $cliente->Email_txf}            <div class="telefone mt-5">              Email: <a href="mailto:{$cliente->Email_txf}">{$cliente->Email_txf}</a>            </div>            {/if}          </div>          <div class="visite text-left mt-30">            <h2 class="fz-18 fw-700 ff-secondary">              Visite-nos            </h2>            <div class="mt-5">              {$cliente->Endereco_txf}, {$cliente->Numero_txf} - Bairro {$cliente->Bairro_txf} - {$cliente->Cidade_txf},              {$cliente->Estado_sel}            </div>          </div>          <hr class="mt-30 mb-30"/>          <div class="mt-30">            <button type="button" class="btn-lands btn-secondary fz-16" data-toggle="modal"                    data-target="#exampleModal">              Entre em contato conosco            </button>          </div>        </div>      </div>    </div>  </div></main><footer id="rodape" class="fz-12 lh-15">  <div class="bottom bg-secondary">    <div class="container">      <div class="row">        <div class="col-lg-8">          <div class="direitos ff-secondary align-center">            Copyright © {date("Y")} {$cliente->Fantasia_txf}. Todos os direitos reservados          </div>        </div>        <div class="col-lg-4">          <div class="assinatura text-center text-lg-right">            <div class="d-inline-block va-middle ff-secondary fz-12 fw-700 pr-15">              Feito por:            </div>            <a              title="Acesse o site da Lands - Agência Web"              href="https://landsagenciaweb.com.br" target="_blank"            ></a>          </div>        </div>      </div>    </div>  </div></footer><!-- Modal --><div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"     aria-hidden="true">  <div class="modal-dialog" role="document">    <div class="modal-content">      <div class="modal-header">        <h5 class="modal-title" id="exampleModalLabel">Entre em contato</h5>        <button type="button" class="close" data-dismiss="modal" aria-label="Close">          <span aria-hidden="true">&times;</span>        </button>      </div>      <div class="modal-body pa-0">        <div id="contato" class="pr-40 pl-40 pt-40 pb-40">          <section class="listagem listagem-1">            <form id='formulario_contato'>                <input name="Destinatario_txf" type="hidden" id="destinatario" value="{$cliente->Email_txf}"/>                <div class="form-group">                  <input name="Nome_txf" placeholder="Seu nome" type="text" class=" form-control" value=""                         required>                </div>                <div class="form-group">                  <input name="Email_txf" placeholder="Email" type="email" class="form-control" value=""                         required>                </div>                <div class="form-group">                  <input name="Telefone_txf" placeholder="Telefone" type="phone" class=" form-control tel"                         value="">                </div>                <div class="form-group paddbt">                              <textarea placeholder="Mensagem" name="Mensagem_txa" placeholder="Mensagem" id="mensagem"                                        class="form-control" required></textarea>                </div>                <input id='lands_id' value="{$app->Lands_id}" name="Lands_id" type="hidden"/>                <input type="submit" name="enviar" value="Enviar" id="submit"                       class="btn-lands btn-sm btn-secondary btn-block"/>            </form>          </section>        </div>      </div>    </div>  </div></div>{include file=$CAMINHO_TPL|cat:'blocos/global/scripts.tpl'}<script type="text/javascript">  /* attach a submit handler to the form */  $("#formulario_contato").submit(function (event) {    msg = $("#mensagem").val();    if (msg == 'Mensagem') {      alert('Escreva uma mensagem');      return false;    }    /* stop form from submitting normally */    event.preventDefault();    /*clear result div*/    $("#retorno").html('');    /* get some values from elements on the page: */    var values = $(this).serialize();    /* Send the data using post and put the results in a   div */    $.ajax({      url: "{$app->Url_cliente}post/enviar_contato",      type: "post",      data: values,      success: function (data) {        /* alert("success");  */        $("#retorno").html(data);      },      error: function () {        $("#retorno").html('Erro ao gravar contato na base.');      }    });  });</script></body></html>