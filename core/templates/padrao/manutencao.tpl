<!doctype html><html lang="pt-br"><head>  <base href="{$app->Url_cliente}">  <link rel="shortcut icon" href="{$assets}imagens/favicon.ico" type="image/x-icon"/>  {include file=$CAMINHO_TPL|cat:'blocos/global/meta_tags.tpl'}  {*ESTILOS*}  {include file=$CAMINHO_TPL|cat:'blocos/global/styles.tpl'}  {$app_json=json_encode($app)}  <script>    window.app = '{$app_json}';    window.appUrl = '{$app->Url_cliente}';    window.utils = {      paginaAtual: '{$pagina_atual}',      painel: '{$painel}',      assets: '{$assets}',      segmentos: ['{$pagina_atual}', '{$segment2}', '{$segment3}', '{$segment4}']    };  </script>  {$Scripts_header_txa}</head><body class="bg-body-light"><main class="">  <div class="container">    <div class="row justify-content-center">      <div class="col-12 col-lg-6 pt-50">        <div class="box-1 bs-1 br-2 pl-40 pr-40 pt-40 pb-40 text-center">          <div class="icone fz-80 text-primary lh-0">            <i class="fad fa-construction"></i>          </div>          <h1 class="title fw-700 fz-32 mt-20">            Este site está em manutenção, voltaremos em breve!          </h1>          <hr class="mt-30 mb-30"/>          <div class="contato text-left">            <h2 class="fz-18 fw-700 ff-secondary">              Contato            </h2>            {if $cliente->Telefone_txf}            <div class="telefone mt-5">              Telefone: {$cliente->Telefone_txf}            </div>            {/if}            {if $cliente->Email_txf}            <div class="telefone mt-5">              Email: <a href="mailto:{$cliente->Email_txf}">{$cliente->Email_txf}</a>            </div>            {/if}          </div>          <div class="visite text-left mt-30">            <h2 class="fz-18 fw-700 ff-secondary">              Visite-nos            </h2>            <div class="mt-5">              {$cliente->Endereco_txf}, {$cliente->Numero_txf} - Bairro {$cliente->Bairro_txf} - {$cliente->Cidade_txf}, {$cliente->Estado_sel}            </div>          </div>        </div>      </div>    </div>  </div></main></body></html>