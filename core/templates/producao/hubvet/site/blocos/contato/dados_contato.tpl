<div class="dados">  {if $telefones}  <div class="telefones">    <h3 class="fz-16 fw-400 d-inline">      Telefones:    </h3>    <ul class="d-inline telefones-lista">      {foreach from=$telefones item=telefone key=key}      {if $telefone->Tipo_sel != 'WHATSAPP'}      <li class="telefone d-inline">        <span class="numero">          {if key > 0}, {/if}({$telefone->Ddd_txf}) {$telefone->Numero_txf}        </span>      </li>      {/if}      {/foreach}    </ul>  </div>  {/if}  {if $emails}  <div class="emails">    <h3 class="fz-16 fw-400 d-inline">      Emails:    </h3>    <ul class="emails-lista d-inline">      {foreach from=$emails item=email key=key}      <li class="email d-inline">        <span itemprop="email">          {if key > 0}, {/if}{$email->Email_txf}        </span>      </li>      {/foreach}    </ul>  </div>  {/if}  {if $whats[0]}  <div class="mt-30">    <strong>Se desejar entre em contato pelo WhatsApp</strong>    <button      class="btn-lands btn-secondary bs-0 mt-25"      onclick="document.querySelector('.lands-whatsapp-fab').click()"    >      <i class="fab fa-whatsapp va-middle fz-18"></i>      <span class="pl-10 va-middle">Entrar em contato pelo WhatsApp</span>    </button>  </div>  {/if}  <div class="redes mt-45">    {include file=$CAMINHO_TPL|cat:'blocos/global/redes_sociais_lista.tpl'}  </div></div>