{$tipo=$tipo|default:0}{if $tipo == 1}{$outline = 'form-outline form-dark'}{/if}<form class="form-contato" onsubmit="return false">  <div class="form-lands-group">    <input placeholder="{trans('form_nome')}*" class="form-lands {$outline}" name="Nome_txf" type="text" required/>  </div>  <div class="form-lands-group">    <input placeholder="{trans('form_email')}*" class="form-lands {$outline}" name="Email_txf" type="text" required/>  </div>  <div class="form-lands-group">    <input placeholder="{trans('form_telefone')}*" class="form-lands phone-mask {$outline}" name="Telefone_txf" type="text" required/>  </div>  <div class="form-lands-group">    <select name="Persona_sel" class="d-block custom-select cursor-pointer form-lands">      <option disabled selected>        {trans('form_contato_selecione')}      </option>      {foreach from=$labels item=label}      <option value="{$label->Nome_url}">        {$label->Nome_tit}      </option>      {/foreach}    </select>  </div>  <div class="form-lands-group">    <textarea style="min-height: 140px;" placeholder="{trans('form_mensagem')}" name="Mensagem_txa"              class="form-lands {$outline}"></textarea>  </div>  <input name="Tipo_contato_txf" type="hidden" value="{$tipo_contato|default:'CONTATO'}"/>  <input name="Destinatario_txf" type="hidden" value="{$emails[0]->Email_txf}"/>  <input id='lands_id' value="{$app->Lands_id}" name="Lands_id" type="hidden"/>  <button class="btn-lands btn-primary btn-lg btn-block" type="submit">    {trans('form_enviar')}  </button></form>