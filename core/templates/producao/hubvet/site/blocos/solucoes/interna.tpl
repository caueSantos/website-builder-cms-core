<main id="servico">  {$servico=$servicos[0]}  {$imagens=get_object($servico->Imagens, 'Campo_sel', '==', 'Imagens_ico')}  {$titulo='<strong>'|cat:$servico->Nome_tit|cat:'</strong>'}  {$subtitulo=titulo('servicos_interna', 'sub', $titulos)}  {include file=$CAMINHO_TPL|cat:'blocos/global/head_interna.tpl'}  <div id="wrap" class="pt-40 pt-lg-20">    <div class="container pb-60">      <div class="row justify-content-center">        <div class="col-12 col-lg-8 text-center text-lg-left">          {if $imagens[0]}          <section class="secao-imagem">            <div class="aspect aspect-21-9">              <figure class="imagem aspect-item">                <img alt="{$servico->Nome_tit}" src="{$painel}{$imagens[0]->Caminho_txf}" class="img-fit"/>              </figure>            </div>          </section>          {/if}          {if $servico->Texto_txa}          <div class="texto fz-18 lh-18 mt-25 pr-lg-100">            {$servico->Texto_txa}          </div>          {/if}          {if $servico->Descricao_detalhada_txa}          <section class="caracteristicas-solucao mt-50 mt-lg-60">            <h3 class="title fz-22 fw-600">              Descrição detalhada            </h3>            <div class="texto mt-15 lh-18 fz-18">              {$servico->Descricao_detalhada_txa}            </div>          </section>          {/if}        </div>      </div>    </div>    {if $imagens[1]}    {include file=$CAMINHO_TPL|cat:'blocos/solucoes/galeria_imagens.tpl'}    {/if}    {if $servico->Videos[0]}    {include file=$CAMINHO_TPL|cat:'blocos/solucoes/galeria_videos.tpl'}    {/if}  </div>  {include file=$CAMINHO_TPL|cat:'blocos/global/noticias.tpl'}</main>