{if $sobre_linha_tempo[0]}<section id="sobre-linha-tempo" class="sobre-linha-tempo pt-50 pb-50 pt-md-100 pb-md-40">  <div class="container">    <div class="row justify-content-center">      <div class="col-md-6">        <div class="title-group text-center">          <h1 class="title fz-36 fw-400 lh-12 text-primary">            {titulo('sobre_interna_linha', 'tit', $titulos)}          </h1>          {if titulo('sobre_interna_linha', 'sub', $titulos)}          <div class="texto fz-16 mt-10 lh-15">            {titulo('sobre_interna_linha', 'sub', $titulos)}          </div>          {/if}        </div>      </div>    </div>    <div class="row mt-40">      {$linha_count = count($sobre_linha_tempo)}      {foreach from=$sobre_linha_tempo item=linha key=key}      <div class="col-12 linha">        <div class="row">          <div class="col-12 col-md-2 pb-40">            <div class="pr-md-20 align-center text-right ano">              <div class="d-inline-block va-middle fz-24 title text-dark fw-700 pr-10">                {$linha->Ano_txf}              </div>              <div class="d-inline-block va-middle dot-warp">                <div class="dot bg-accent"></div>              </div>            </div>            {if $key + 1 != $linha_count}            <div class="bg-fake pr-md-35 linha-h-wrap">              <div class="linha-h ml-auto"></div>            </div>            {/if}          </div>          <div class="col-12 col-md-7 pb-40">            <div class="linha-conteudo bg-dark-grey br-1 text-white pl-30 pr-30 pt-30 pb-30">              <div class="title fz-24 fw-700">                {$linha->Titulo_txf}              </div>              <div class="descricao fz-16 mt-20">                {$linha->Texto_txa}              </div>            </div>          </div>          <div class="col-12 col-md-3 pl-md-70 pb-40">            <div class="align-center">              {if $linha->Imagens[0]}              <img src="{$painel}{$linha->Imagens[0]->Caminho_txf}" style="max-height: 60px; max-width: 100%"/>              {/if}            </div>          </div>        </div>      </div>      {/foreach}    </div>  </div></section>{/if}