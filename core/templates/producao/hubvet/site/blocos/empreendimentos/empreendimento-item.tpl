<article class="item-empreendimentos text-center text-lg-left">  <div class="bg-fake">    <div class="container fill-height">      <div class="row fill-height mx-0">        <div class="col-lg-10 fill-height" style="box-shadow: 0px 8px 30px rgba(0, 0, 0, 0.05);"></div>      </div>    </div>  </div>  <div class="container pb-60 pt-60">    <div class="row">      <div class="col-txt col-lg-5 offset-lg-1 order-2 order-lg-0 pt-30 pt-lg-0">        <div class="text-layer align-center">          <div class="text-body-tertiary fz-18 title fw-700">            Empreendimentos          </div>          <div class="title fz-32 fw-700 lh-12 text-primary mt-10">            {$empreendimento->Nome_tit}          </div>          {if $empreendimento->Descricao_curta_txa}          <div class="texto fz-16 fw-400 lh-15 mt-30 text-body-primary">            {$empreendimento->Descricao_curta_txa}          </div>          {/if}          <div class="botao mt-35">            <a href="{$app->Url_cliente_linguagem}empreendimentos/{$empreendimento->Nome_url}"               class="btn-lands btn-outline pr-lg-60 pl-lg-60">              Saiba mais            </a>          </div>        </div>      </div>      <div class="col-img col-lg-6 order-1 order-lg-0">        {$imagem=$assets|cat:'imagens/indisponivel-quadrada.png'}        {if $empreendimento->Imagens[0]}        {$imagem=$painel|cat:$empreendimento->Imagens[0]->Caminho_txf}        {/if}        <div class="aspect aspect-4-3 br-2 overflow-hidden bg-body-light">          <figure class="aspect-item image-layer">            <img alt="{strip_tags($empreendimento->Titulo_txf)}"                 class="img-fit"                 title="{strip_tags($empreendimento->Titulo_txf)}"                 src="{$imagem}"/>          </figure>        </div>      </div>    </div>  </div></article>