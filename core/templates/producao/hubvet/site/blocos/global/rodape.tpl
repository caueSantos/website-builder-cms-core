{$tipo=2}{include file=$CAMINHO_TPL|cat:'blocos/global/experimente-secao.tpl'}<footer id="rodape" class="fz-12 lh-15 text-white bg-dark-grey">  <div class="top">    <div class="container">      <div class="pt-50 pt-md-90 pb-50 pb-md-70">        <div class="row">          <div class="col-lg-3 col-logo">            <div class="logo" style="width: 176px">              <a href="{$app->Url_cliente_linguagem}" title="Acesse a página inicial">                <img width="100%" itemprop="image" class="img-fluid" src="{$assets}imagens/logo-rodape.png"                     alt="Logo {$cliente->Fantasia_txf}">              </a>            </div>          </div>          <div class="col-md d-none d-md-block"></div>          <div class="col-md-auto">            <div class="align-center fz-16 fz-400">              {trans('nos_acompanhe_redes')}:            </div>          </div>          <div class="col-md-auto">            <div class="fz-24 align-center">              {include file=$CAMINHO_TPL|cat:'blocos/global/redes_sociais_lista.tpl'}            </div>          </div>        </div>      </div>      <hr class="mt-0 mb-0"/>    </div>  </div>  <div class="middle pt-50 pt-md-70 pb-50 pb-md-60">    <div class="container">      <div class="row">        <div class="col-md-8">          <div class="row">            {$menu_topo=junta_registros($menu_topo, 'Nome_url', $menu_topo_itens, 'Menu_sel', 'Itens')}            {$menu_topo = get_object($menu_topo, 'Mostra_rodape_sel', '=', 'SIM')}            {foreach from=$menu_topo item=menu}            {if $menu->Itens[0]}            <div class="col-md">              <h3 class="fz-14 fw-700">                {$menu->Nome_tit}              </h3>              <ul class="menu fz-14">                {foreach from=$menu->Itens item=menu_item}                <li class="nav-item mt-30">                  <a                    class="nav-link {if $pagina_atual == $menu_item->Nome_url}active{/if}"                    title="{$menu_item->Nome_txf}"                    href="{gera_link($menu_item->Botao_link_txf, true)}"                    target="{if $menu_item->Target_sel}{$menu_item->Target_sel}{else}_self{/if}"                  >                    {$menu_item->Nome_txf}                  </a>                </li>                {/foreach}              </ul>            </div>            {/if}            {/foreach}          </div>        </div>        <div class="col-md-4">          <!--ATENDIMENTO-->          <div class="bg-dark-grey-2 text-white pt-20 pb-20 pl-30 pr-30 br-1">            <div class="row">              <div class="col-auto">                <div                  class="aspect-item bg-dark-grey fz-24 text-center align-center"                  style="height: 50px; width: 50px"                >                  <div class="align-center lh-1">                    <i class="far fa-comment-alt-lines"></i>                  </div>                </div>              </div>              <div class="col-sm">                <div class="align-center fz-14">                  <div class="title fw-700">                    {trans('email')}                  </div>                  <div class="texto mt-5">                    {$emails[0]->Email_txf}                  </div>                </div>              </div>            </div>          </div>          <!--TELEFONE-->          <div class="bg-dark-grey-2 text-white pt-20 pb-20 pl-30 pr-30 br-1 mt-15">            <div class="row">              <div class="col-auto">                <div                  class="aspect-item bg-dark-grey fz-24 text-center align-center"                  style="height: 50px; width: 50px"                >                  <div class="align-center lh-1">                    <i class="fal fa-phone-alt"></i>                  </div>                </div>              </div>              <div class="col-sm">                <div class="align-center fz-14">                  <div class="title fw-700">                    {trans('telefone')}                  </div>                  <div class="texto mt-5">                    ({$telefones[0]->Ddd_txf}) {$telefones[0]->Numero_txf}                  </div>                </div>              </div>            </div>          </div>          <!--ENDEREÇO-->          <div class="bg-dark-grey-2 text-white pt-20 pb-20 pl-30 pr-30 br-1 mt-15">            <div class="row">              <div class="col-auto">                <div                  class="aspect-item bg-dark-grey fz-24 text-center align-center"                  style="height: 50px; width: 50px"                >                  <div class="align-center lh-1">                    <i class="fal fa-map-marker-alt"></i>                  </div>                </div>              </div>              <div class="col-sm">                <div class="align-center fz-14">                  <div class="title fw-700">                    {trans('endereco')}                  </div>                  <div class="texto mt-5">                    {$enderecos[0]->Endereco_txf} - {$enderecos[0]->Bairro_txf},                    {$enderecos[0]->Cidade_txf} - {$enderecos[0]->Estado_txf},                    {$enderecos[0]->Cep_txf}                  </div>                </div>              </div>            </div>          </div>        </div>      </div>    </div>  </div>  <div class="bottom bg-dark-grey pt-50 pt-md-60 pb-15">    <div class="container">      <div class="row">        <div class="col-lg-8 text-body-secondary">          <div class="direitos align-center">            {$enderecos[0]->Endereco_txf} - {$enderecos[0]->Bairro_txf},            {$enderecos[0]->Cidade_txf} - {$enderecos[0]->Estado_txf},            {$enderecos[0]->Cep_txf}          </div>        </div>        <div class="col-lg-4">          <div class="text-center text-lg-right fz-12">            <div class="font-rodape ml-auto">              {trans('site_criado_com')}              <i class="fas fa-heart mx-1" style="color: red;"></i>              {trans('pela')}              <a                href="{gera_link(config('site_lands_link'))}"                target="_blank"                class="fw-700 text-white"              >                {trans('lands_agencia_nome')}              </a>            </div>          </div>        </div>      </div>    </div>  </div>  <div class="links-rodape pt-20 pb-30 text-body-secondary">    <div class="container">      <div class="row justify-content-center">        <div class="col-12 col-md-auto text-center">          <ul class="fw-700 fz-12">            <li class="d-block d-md-inline-block pl-md-30 pr-md-30">              ©{$cliente->Fantasia_txf}            </li>            <li class="d-block d-md-inline-block pl-md-30 pr-md-30">              <a class="d-block text-body-secondary text-primary-hover" href="{gera_link(config('politicas_link'))}" target="_blank">                {trans('politicas')}              </a>            </li>            <li class="d-block d-md-inline-block pl-md-30 pr-md-30">              <a class="d-block text-body-secondary text-primary-hover" href="{gera_link(config('termos_link'))}" target="_blank">                {trans('termos')}              </a>            </li>          </ul>        </div>      </div>    </div>  </div></footer><div id="retorno"></div>