<div id="orientacoes-ajax">  {foreach from=$orientacoes->registros item=item key=key}  {if $item->Texto_txa}  <div class="row">    <div class="col-12">      <div        class="orientacao d-block text-center bg-primary text-white br-1        overflow-hidden mb-30 pt-70 pb-60 pl-50        pr-50 pl-lg-120 pr-lg-120">        {$fundo=get_object($item->Imagens, 'Campo_sel', '=', 'Imagem_fundo_ico')}        <div class="bg-fake">          {if $fundo[0]}          <img class="img-fit" src="{$painel}{$fundo[0]->Caminho_txf}"/>          {else}          <img class="img-fit" src="{$assets}imagens/caminhao.jpg"/>          {/if}        </div>        <div          class="bg-fake"          style="background: linear-gradient(110deg, rgba(255, 27, 69, .9) 50%, rgba(255, 27, 69, .2) 100%);"        ></div>        <div          class="bg-fake"          style="background: linear-gradient(330deg, rgba(255, 27, 69, .7) 00%, rgba(255, 27, 69, 0) 40%);"        ></div>        <div          class="bg-fake"          style="background: linear-gradient(360deg, rgba(255, 27, 69, 1) 0%, rgba(255, 27, 69, 0) 100%);"        ></div>        {$icone=get_object($item->Imagens, 'Campo_sel', '=', 'Icone_ico')}        {if $icone[0]}        <figure class="imagem mb-30" style="height: 80px">          <img            style="width: auto; height: auto; max-width: 100%; max-height: 100%"            alt="{strip_tags($item->Titulo_txa)}"            src="{$painel}{$icone[0]->Caminho_txf}"          />        </figure>        {/if}        <div class="title fz-22">{$item->Titulo_txa}</div>        <div class="desc fz-14">{$item->Dica_txf}</div>        <div          id="orientacao-{$item->Id_int}"          class="texto collapse"        >          <div class="pt-30">            {$item->Texto_txa}          </div>        </div>        <div class="botao mt-20">          <div            data-toggle="collapse"            data-target="#orientacao-{$item->Id_int}"            class="btn-lands btn-white btn-sm br-1 tt-upper">            Acessar orientações          </div>        </div>      </div>    </div>  </div>  {/if}  {/foreach}  {$paginacao=$orientacoes->paginacao}  {include file=$CAMINHO_TPL|cat:'blocos/global/paginacao.tpl'}</div>