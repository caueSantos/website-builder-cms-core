<main id="central">  {$titulo='<strong>'|cat:titulo('internal_central', 'tit', $titulos)|cat:'</strong>'}  {$subtitulo=titulo('internal_central', 'sub', $titulos)}  {include file=$CAMINHO_TPL|cat:'blocos/global/head_interna.tpl'}  <div id="wrap" class="pb-20">    <section class="container pt-30 pb-40">      <div class="row justify-content-center">        <div class="col-12 col-lg-8">          <div class="text-center">            <h3 class="title fz-24 fz-lg-32 fw-400 lh-1 text-primary">              {titulo('interna_central_orientacao', 'tit', $titulos)}            </h3>            {if titulo('interna_central_orientacao', 'sub', $titulos)}            <div class="texto fz-14 mt-10 lh-15">              {titulo('interna_central_orientacao', 'sub', $titulos)}            </div>            {/if}          </div>          <div id="orientacoes-container" class="pt-50">            {include file=$CAMINHO_TPL|cat:'ajax/orientacoes.tpl'}          </div>        </div>      </div>    </section>    <section class="bg-secondary text-white pt-50 pt-lg-80 pb-50 pb-lg-80">      <div class="container">        <div class="row justify-content-center">          <div class="col-12 col-lg-6">            <div class="text-center">              <div class="fz-18">                Em caso de dúvidas              </div>              <h3 class="title fz-24 fz-lg-48 fw-700 lh-1">                entre em contato              </h3>              <div class="texto fz-18 mt-10">                Vamos resolver seu problema o mais rápido possível!              </div>            </div>            <div class="mt-30">              {include file=$CAMINHO_TPL|cat:'blocos/central/contato.tpl'}            </div>          </div>        </div>      </div>    </section>    <section class="container pt-50 pt-lg-60">      <div class="row justify-content-center">        <div class="col-12 col-lg-6">          <div class="text-center">            <h3 class="title fz-24 fz-lg-32 fw-400 lh-1 text-primary">              Dúvidas <strong>frequentes</strong>            </h3>            <div class="texto fz-18 mt-10 lh-1">              Vamos resolver seu problema o mais rápido possível!            </div>          </div>          <div id="perguntas-container" class="pt-30">            {include file=$CAMINHO_TPL|cat:'ajax/perguntas.tpl'}          </div>        </div>      </div>    </section>  </div>  {include file=$CAMINHO_TPL|cat:'blocos/global/noticias.tpl'}  {include file=$CAMINHO_TPL|cat:'blocos/global/secao-corretor.tpl'}</main><script>  $(function () {    function carregaConteudo($container, pagina, params = '') {      var loader = '<div class="dot-elastic"></div>';      var height = $container.height();      $.ajax({        url: window.appUrl + 'super_ajax/' + pagina + '?' + params,        type: "get",        beforeSend: function () {          rolagem($container.attr('id'), 200, 300);          $container.html('<div class="text-center pt-50 pb-50" style="height: ' + height + 'px">' + loader + '</div>');        },        success: function (data) {          setTimeout(function () {            $container.html(data);          }, 600);        },        error: function () {          $container.html("Erro ao abrir página");        }      });    }    // carregaConteudo($("#perguntas-container"), 'perguntas');    // carregaConteudo($("#orientacoes-container"), 'orientacoes');    $('body').on('click', '#perguntas-ajax .pagination .page-item a', function (e) {      e.preventDefault();      var $this = $(this), params = new URL($this.attr('href'));      params = params.search.replace('?', '');      carregaConteudo($("#perguntas-container"), 'perguntas', params);    }).on('click', '#orientacoes-ajax .pagination .page-item a', function (e) {      e.preventDefault();      var $this = $(this), params = new URL($this.attr('href'));      params = params.search.replace('?', '');      carregaConteudo($("#orientacoes-container"), 'orientacoes', params);    });  });</script>