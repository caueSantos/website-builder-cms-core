<main id="contato">  <div class="pe-none" style="height: 500px">    <img src="{$assets}imagens/contato.png" class="img-fit"/>  </div>  <div id="wrap" class="pb-60 pb-lg-110">    <div class="container">      <div class="row">        <div class="col-lg-6">          <div class="box-1 br-2 bs-1 pt-40 pt-lg-60 pb-40 pb-lg-100 pl-25 pl-lg-50 pr-25 pr-lg-50 text-center"               style="margin-top: -370px">            <div class="pe-none">              <img src="{$assets}imagens/message.png"/>            </div>            <h1 class="title fw-700 text-body-quaternary fz-32 mt-25" style="letter-spacing: -0.04rem">              {titulo('interna_contato_form', 'tit', $titulos)}            </h1>            {if titulo('interna_contato_form', 'sub', $titulos)}            <div class="texto fz-16 lh-15 mt-5">              {titulo('interna_contato_form', 'sub', $titulos)}            </div>            {/if}            <div class="mt-30">              {include file=$CAMINHO_TPL|cat:'blocos/contato/contato.tpl'}            </div>          </div>        </div>        <div class="col-lg-5 offset-lg-1 text-center text-lg-left">          <div class="pt-50">            <h1 class="title fw-700 text-body-quaternary fz-24">              {titulo('interna_contato_dados', 'tit', $titulos)}            </h1>            {if titulo('interna_contato_dados', 'sub', $titulos)}            <div class="texto fz-16 lh-15">              {titulo('interna_contato_dados', 'sub', $titulos)}            </div>            {/if}            <div class="mt-20">              {include file=$CAMINHO_TPL|cat:'blocos/contato/dados_contato.tpl'}            </div>          </div>        </div>      </div>    </div>  </div></main>